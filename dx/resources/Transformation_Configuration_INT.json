                                },
                                {
                                    "column": [
                                        "AddressID"
                                    ],
                                    "value": -1
                                }
                            ]
                        }
                    },
                    {
                        "TransformationType": "Pseudonymization",
                        "Params": {
                            "columns": [
                                "AddressLine1",
                                "City"
                            ]
                        }
                    }
                ]
            },
            {
                "DatabaseName": "Application_MEF_LandingSQL",
